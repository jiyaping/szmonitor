{
  "name": "sza monitor",
  "description": "monitor all node",
  "version": "0.0.1",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
      "name": "click to change status"
  },
  "permissions":[
	"http://*/",
	"notifications"
  ],
  "options_page": "options.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
